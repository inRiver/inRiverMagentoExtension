<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <inriver>
            <import>
                <debug>0</debug>
                <behavior>append</behavior>
                <path_csv>inriver/import/csv</path_csv>
                <inriver_api_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <maximum_allowed_error>99999</maximum_allowed_error>
            </import>
        </inriver>
    </default>
</config>
